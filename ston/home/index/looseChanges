          not
          ifTrue: [ 
            | add |
            add := false.
            cat := (beh categoryOfSelector: selector) asString asLowercase.
            (cat beginsWith: \'*\')
              ifTrue: [ 
                packagePrefixes
                  detect: [ :prefix | cat beginsWith: prefix ]
                  ifNone: [ add := true ] ]
              ifFalse: [ add := true ].
            add
              ifTrue: [ 
                | ar persistentMethodDict sessionMethod |
\t\t\t\t\tar := {beh.
