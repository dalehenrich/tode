TDSmalltalkLeafNode{#name:'combineGenerator',#contents:'| formatter combineWith negateWith |
combineWith := \'combineWith: aGsQueryOptions \'.
negateWith := \'negateWith: aGsQueryOptions \'.
#(#\'applyDeMorgansLaws\' #\'cacheQueryResult\' #\'consolidateEnumerablePredicates\' #\'consolidateRangePredicates\' #\'normalizePredicates\' #\'removeRedundantPredicates\' #\'reorderPredicates\' #\'transformCommonPaths\')
  do: [ :ivSym | 
    | iv |
    iv := ivSym asString.
    combineWith := combineWith , \' self \' , iv
      , \' ifTrue: [ aGsQueryOptions set\' , iv capitalized , \' ]. \'.
    negateWith := negateWith , \' self \' , iv
      , \' ifTrue: [ aGsQueryOptions clear\' , iv capitalized , \' ]. \' ].
combineWith := combineWith , \' ^ aGsQueryOptions\'.
negateWith := negateWith , \' ^ aGsQueryOptions\'.
formatter := [ :aString | 
(RBParser parseMethod: aString asString onError: [ :err :pos | ^ aString ])
  formattedCode ].
GsQueryOptions
  compile: (formatter value: combineWith);
  compile: (formatter value: negateWith)',#creationTime:nil,#modificationTime:nil}