TDScriptLeafNode{#name:'loadConfig',#contents:'[ :topez :objIn :tokens :windowId | 
| projectName version loads description status statusNode loaded |
\"/home/bin/loadConfig Seaside 3.0.7.1 repo default \"
projectName := tokens at: 2.
version := tokens at: 3.
loads := \'default\'.
description := tokens at: 4.
tokens size > 4
  ifTrue: [ loads := tokens copyFrom: 5 to: tokens size ].
Metacello new
  configuration: projectName;
  repository: description;
  get.
GsDeployer deploy: [loaded := Metacello new
  configuration: projectName;
  version: version;
  repository: description;
  onConflict: [ :ex | ex allow ];
  onWarning: [ :ex | 
        Transcript
          cr;
          show: \'WARNING: \' , ex description.
        ex resume ];
  load: loads].
statusNode := topez
  lookup: \'status\'
  ifAbsent: [ 
    topez currentNode
      addChildNode:
        (TDTxtLeafNode new
          name: \'status\';
          yourself) ].
statusNode
  contents:loaded printString.
loaded ]'}