TDScriptLeafNode{#name:'load',#contents:'[ :topez :objIn :tokens :command | 
| opts args repo loads projectName project baseline configuration |
\"for help: ./load -h\"
command
  getOptsMixedLongShort:
    {#(\'baseline\' $b #\'required\').
    #(\'configuration\' $c #\'required\').
    #(\'help\' $h #\'none\').
    #(\'load\' $l #\'required\').
    #(\'project\' $p #\'required\').
    #(\'repo\' $r #\'required\')}
  optionsAndArguments: [ :options :operands | 
    opts := options.
    args := operands ].
projectName := \'Example\'.
baseline := true.
opts
  at: \'baseline\'
  ifAbsent: [ baseline := false ]
  ifPresent: [ :pName | 
    projectName := pName.
    baseline := true ].
opts
  at: \'configuration\'
  ifAbsent: [ configuration := false ]
  ifPresent: [ :pName | 
    projectName := pName.
    configuration := true ].
opts
  at: \'project\'
  ifAbsent: [ project := false ]
  ifPresent: [ :pName | 
    projectName := pName.
    project := true ].
opts
  at: \'load\'
  ifAbsent: [ loads := \'default\' ]
  ifPresent: [ :loadList | loads := loadList findTokens: \' \' ].
opts
  at: \'repo\'
  ifAbsent: [ 
    (topez lookup: \'repo\' ifAbsent: [  ])
      ifNotNil: [ :node | repo := node dotSelfObject ] ]
  ifPresent: [ :description | 
    | mc |
    mc := topez toolInstanceFor: \'mc\'.
    repo := mc resolveRepositoryReference: description ].
opts
  at: \'help\'
  ifAbsent: [ 
    | metacello get |
    metacello := Metacello new.
    configuration
      ifTrue: [ metacello configuration: projectName ]
      ifFalse: [ 
        project
          ifTrue: [ metacello project: projectName ]
          ifFalse: [ metacello baseline: projectName ] ].
    metacello repository: repo description.
    get := metacello copy.
    get get.

    metacello 
onConflict: [:ex | ex allow];
load: loads ]
  ifPresent: [ :ignored | 
    TDManPage
      viewManPage:
        \'NAME
  load - load sript utility template
SYNOPSIS
  load [-h|--help]
       [--baseline=<baseline-name>]| 
           [--configuration=<configuration-name>]|[--project=<project-name>]
       [--repo=<repository-reference>]
       [--load=<load-list>]
DESCRIPTION

  default <project-name> is for baseline Example.
  default <repository-reference> is ./repo.
  default <load-list> is default.
EXAMPLES
  ./load
  ./load --baseline=Example --repo=@exampleDir --load=Tests
  ./load --baseline=External --repo=@externalDir --load=Tests
  ./load --baseline=Sample --repo=@sampleDir --load=Tests
  ./load --load=Core
  ./load --load=`Core Tests`
  ./load --repo=@repo
  ./load --repo=filetree://opt/git/PharoCompatibility/repository
\'
      topez: topez ] ]',#creationTime:DateAndTime['2013-11-17T15:45:00.30549788475037-08:00'],#modificationTime:DateAndTime['2013-11-17T16:55:43.77922606468201-08:00']}